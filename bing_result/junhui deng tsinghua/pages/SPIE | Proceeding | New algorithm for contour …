                function () {
                    var $element1 = $('#dvsliderright');
                    var $element2 = $('#dvsliderleft');
                    $element1.hide();
                    $element1.hide();
                });

            $("#globalHeader_divUmbrellaNavigationBar > ul > li").eq(2).mouseleave(
                function () {
                    var $element1 = $('#dvsliderright');
                    var $element2 = $('#dvsliderleft');

                    if ($("#dvsliderleft:visible").length) {
                        $element2.show();
                        $element1.hide();
                    } else {
                        $element1.show();
                    }

                });

        });

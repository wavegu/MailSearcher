       <!--
        var VIEW_MORE_TEXT_SECTIONS = '[+] View More'
        var HIDE_TEXT_SECTIONS = '[-] View Fewer'
        var COOKIE_NAME_SECTIONS = 'relatedSectionsWidgetViewMore'
        var VIEW_MORE_SECTION_CLASS_SECTIONS = '.viewMoreSectionsSection'
        var CONTENT_SECTION_CLASS_SECTIONS = '.relatedSectionsContent'
        var VIEW_MORE_LINK_ID_SECTIONS = '#aShowMoreSections'

        function SectionDatafromServer(args) {
            var obj = JSON.parse(args, "");
            // alert('SectionDatafromServer');
            $(CONTENT_SECTION_CLASS_SECTIONS).html(obj._ContentHTML);

            //run the function that checks the state of View More
            restoreViewMoreSectionsOnLoad();
        }

        // function that maintains View More state, from a cookie
        function checkViewMoreSectionsCookie() {
            // fetch the cookie value (View More open or not)
            if (!$.cookie(COOKIE_NAME_SECTIONS)) return 0;
            var cookie = $.cookie(COOKIE_NAME_SECTIONS);
            //alert('check cookie Section: ' + cookie);
            return cookie;
        }

        function expandViewMoreSections() {
            //show the div
            $(VIEW_MORE_SECTION_CLASS_SECTIONS).show();
            //change the link text
            $(VIEW_MORE_LINK_ID_SECTIONS).text(HIDE_TEXT_SECTIONS);
            // alert('expand Sections: ' + $(VIEW_MORE_LINK_ID).text());
            // Set the cookie to 1 for expanded
            $.cookie(COOKIE_NAME_SECTIONS, '1', { expiresAt: new Date(2100, 1, 1) });
        }

        function collapseViewMoreSections() {
            //hide the div
            $(VIEW_MORE_SECTION_CLASS_SECTIONS).hide();
            //change the link text
            $(VIEW_MORE_LINK_ID_SECTIONS).text(VIEW_MORE_TEXT_SECTIONS);
            // alert('collapse Sections: ' + $(VIEW_MORE_LINK_ID).text());
            // Set the cookie to 0 for collapsed
            $.cookie(COOKIE_NAME_SECTIONS, '0', { expiresAt: new Date(2100, 1, 1) });
        }

        function restoreViewMoreSectionsOnLoad() {
            //  alert('restore Sections');
            switch (checkViewMoreSectionsCookie()) {
                case '1': expandViewMoreSections();
                    break;
                default: collapseViewMoreSections();
            }
        }

        function switchViewMoreSectionsOnLoad() {
            //    alert('switch Sections');
            switch (checkViewMoreSectionsCookie()) {
                case '0': expandViewMoreSections();
                    break;
                default: collapseViewMoreSections();
            }
        }

        $(document).ready(function () {
            $('#aShowMoreSections').click(function () {
                switchViewMoreSectionsOnLoad();
            })

            //on initial load, check to see if expanded or collapsed
            restoreViewMoreSectionsOnLoad();
        })
        //-->
    </script>
   

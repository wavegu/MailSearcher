    <script type="text/javascript">
        var whichPanel = "wrapper";
        var mobileFireEvent = "article";
        var count = 0;
        var emailUserAlert = "";
        var emailOnPopupChanged = false;
        var JumpId = '';
        $(document).ready(function () {

            // tracking content references
            $('.crsRef').click(function () {
                var tab = $(this).context.innerText;
                if (!tab) {
                    tab = $(this).context.textContent;
                }
                var key = 'CrossReferenceLink'
                //popuate the input object and call the pagebase method to update the database
                this.UserSessionItem = new UserSessionItem("puntknpwe5lvcjguzpdzgqqw", 0, 0);
                this.ActionType = new ActionType(1, key);
                doTracking(this.ActionType, tab.toString(), "10/28/2015 3:52:54 AM", this.UserSessionItem, (window.location).toString());
            });

            $('.pubmedLink').click(function () {
                var tab = $(this).attr('href');
                var key = 'CrossReferenceLink';
                this.UserSessionItem = new UserSessionItem("puntknpwe5lvcjguzpdzgqqw", 0, 0);
                this.ActionType = new ActionType(1, key);
                doTracking(this.ActionType, tab.toString(), "10/28/2015 3:52:54 AM", this.UserSessionItem, (window.location).toString());
            });

            $('#txtUserAlertEmailAddress').val($('#hfOriginalEmailAddress').val());

            var browserversion = $.browser;

            if (browserversion.mozilla && parseInt(browserversion.version) < 2) {
                $('.saveMyStuff').click(function () {
                    // Don't allow saving in PreviewMode
                    if (previewMode) {
                        alert('Saving is disabled in preview mode.');
                    } else {
                        var _id = this.id;
                        AddItemstoMyStuff(_id);
                    }
                });
            }

            // track toolbox client clicks
            $('.rightpopup li a').click(function () {
                var id = $(this).attr('class');
                var value = id.toString() + ' is Clicked';
                var key = 'ToolBox';
                //popuate the input object and call the pagebase method to update the database
                this.UserSessionItem = new UserSessionItem("puntknpwe5lvcjguzpdzgqqw", 0, "30");
                this.ActionType = new ActionType(1, key);
                if (id == 'tool_pdf linkPDF') {
                    doTracking(this.ActionType, value.toString(), "10/28/2015 3:52:54 AM", this.UserSessionItem, ActionURL, "sync", "0", "0");
                } else {

                    doTracking(this.ActionType, value.toString(), "10/28/2015 3:52:54 AM", this.UserSessionItem, ActionURL);
                }
            });

            $(".tabtracking").click(function () {
                var key = "Tabs";
                var tab = $(this).context.innerText;
                if (!tab) {
                    tab = $(this).context.textContent;
                }
                var value = tab + " was clicked ";
                this.ActionType = new ActionType(1, key);
                this.UserSessionItem = new UserSessionItem("puntknpwe5lvcjguzpdzgqqw", 0, 0);
                doTracking(this.ActionType, value.toString(), "10/28/2015 3:52:54 AM", this.UserSessionItem, ActionURL);
            });
            $(".jumpLinks a").click(function () {
                var thisLink = $(this).index();
                var tab = $(this).context.innerText;
                if (!tab) {
                    tab = $(this).context.textContent;
                }
                var imageClicked = tab + " was clicked ";
                this.ActionType = new ActionType(1, tab);
                this.UserSessionItem = new UserSessionItem("puntknpwe5lvcjguzpdzgqqw", 0, 0)
                doTracking(this.ActionType, imageClicked.toString(), "10/28/2015 3:52:54 AM", this.UserSessionItem, ActionURL);
            });
            $(".popUpTable").click(function () {
                var thisLink = $(this).index();
                var tab = $(this).context.innerText;
                if (!tab) {
                    tab = $(this).context.textContent;
                }
                var imageClicked = tab + " was clicked ";
                this.ActionType = new ActionType(1, tab);
                this.UserSessionItem = new UserSessionItem("puntknpwe5lvcjguzpdzgqqw", 0, 0)
                doTracking(this.ActionType, imageClicked.toString(), "10/28/2015 3:52:54 AM", this.UserSessionItem, ActionURL);

            });
            $(".saveMyStuff").click(function () {
                // Don't track a save to my stuff if this is in preview mode (because saving to my stuff isn't allowed in preview mode)
                if (!previewMode) {
                    var thisLink = $(this).index();
                    var tab = $(this).context.innerText;
                    if (!tab) {
                        tab = $(this).context.textContent;
                    }
                    var id = $(this).attr('id');
                    var imageClicked = tab + " was clicked with id = " + id;
                    this.ActionType = new ActionType(1, tab);
                    this.UserSessionItem = new UserSessionItem("puntknpwe5lvcjguzpdzgqqw", 0, 0)
                    doTracking(this.ActionFilterType, imageClicked.toString(), "10/28/2015 3:52:54 AM", this.UserSessionItem, ActionURL);
                }
            });
        });

        function detailsHandler(href) {
            var hiddenField = document.getElementById('hfArticleId').value;
            //get any other hidden fields and append them.
            href = href + "&articleid=" + hiddenField;
            //then redirect to the revised url
            window.location = href;

        }

        function displaypopup() {

            // show the div content for popup
            $('#displaypopup').attr("style", "displaylock;font-family:Arial;font-weightold;font-size:10pt;");

            $("#displaypopup").dialog({
                position: 'center',
                modal: true,
                height: 150,
                width: 525,
                dialogClass: 'boxshadow',
                closeOnEscape: false,
                overlay: { opacity: 0.5, background: 'gray' },
                close: function (evnt, args) {
                }

            });

        }
        //******************USER ALERT RELATED FUNCTIONS******************
        // User Alerts Callback
        function displayAlertMessage(msg) {
            $(".alertPopupMessage").html(msg);

            // Hide Textbox and Confirm Button?
            $('.UserAlertEmailConfirmation').hide();

            // hide the progress bar after the message is displayed.
            $('.userAlertPopupProgressBar').hide();
        }

        //.........................
        // Trim a string.
        function trim(stringToTrim) {
            return stringToTrim.replace(/^\s+|\s+$/g, "");
        }
        //.........................
        // Capture the new/Changed Email Address when and if the user changes it on User Alerts popup dialog.
        function getNewEmailAddress(o) {
            emailOnPopupChanged = true;
            emailUserAlert = o.value;
        }
        //.........................
        // function that is called for User Alerts Callback
        function callBackUserAlert() {
            // For some if user changed/update value in txtUserAlertEmailAddress field, the new  value was not reflected;
            // As a result, getNewEmailAddress() function is called onblur() of txtUserAlertEmailAddress(widgetUserAlerts user control).
            // Local global variable is set to the new value in onblur().

            // get the email address.
            // if user entered a new email address or updated an existing one, get the value from the local variable.
            // else get it from the textbox itselft.
            var email;
            email = emailOnPopupChanged ? emailUserAlert : $('#txtUserAlertEmailAddress').val();

            if (emailOnPopupChanged) {
                emailOnPopupChanged = false;
            }

            var emailPattern = /\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/;

            // trim the email value
            var emailTrimmed = trim(email);

            // email must have a value.  Stop!!!
            if (emailTrimmed.length == 0) {
                $(".alertPopupMessage").html('Email Address is blank. Please try again.');
                return false;
            }

            // verify if email pattern is valid.
            var regEx = new RegExp(emailPattern);
            var bMatch = regEx.test(emailTrimmed);

            // Email Address doesn't match the pattern. Invalid Email. Stop!!!
            if (bMatch == false) {
                $(".alertPopupMessage").html('Email address must be valid. Ex: name@domain.com');
                return false;
            }

            // show the progress bar...
            $('.userAlertPopupProgressBar').show();

            // initiate the callback
            callBackSaveAlert(emailTrimmed);
        }

        function test(o) { }
        function downloadFile(filename) {
            var url = filename;
            window.location.href = url;
        }

        $(document).ready(function () {
            if ($('.customvalidator').css('visibility') == 'hidden') {
                $('.errorMessageSection').hide();
                $('.customvalidator').css('visibility', 'hidden');
            } else {
                $('.errorMessageSection').show();
                $('.customvalidator').css('visibility', 'visible');
            }

            $('.signInButton').each(function () {
                $(this).click(function () {
                    if ($('.customvalidator').css('visibility') == 'hidden') {
                        $('.errorMessageSection').hide();
                        $('.customvalidator').css('visibility', 'hidden');
                    } else {
                        $('.errorMessageSection').show();
                        $('.customvalidator').css('visibility', 'visible');
                    }
                });
            });

            // adjust open access icon widh doi
            if ($('#scm6MainContent_lblFreeArticle').length) {
                $('.doi .clearfix').addClass('adjustopenAccess');
            }

        });


        $(document).ready(function () {
            // This part bellow is to display BrightCove Vedios
            var feedLink = '';
            var isUserLoggedIn = $get('hfUserHasSubscription').value;
            if (isUserLoggedIn == "1") {
                getBrightCoveID(feedLink);
            }
            var BrightCoveVedioID;
            function getBrightCoveID(url) {
                $('#dvVidPresents').hide();
                $.ajax({
                    type: "POST",
                    url: "proceeding.aspx/getBrightCoveID",
                    data: '{ "FeedURL": "' + url + '" }',
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        var data = (data.hasOwnProperty('d')) ? $.parseJSON(data.d) : data;
                        BrightCoveVedioID = (data.hasOwnProperty('d')) ? data.d.items[0].id : data.items[0].id;
                        var embed = {};
                        embed.h = 505;
                        embed.w = 600;
                        embed.bg = "#ffffff";
                        embed.playerid = "1094074693001";
                        var BCVideo = '<object id="myExperience" class="BrightcoveExperience"><param name="bgcolor" value="' + embed.bg + '" /><param name="width" value="' + embed.w + '" /><param name="height" value="' + embed.h + '" /><param name="playerID" value="' + embed.playerid + '" /><param name="playerKey" value="AQ~~,AAAAoHrMRhk~,T5-k00gMv_uqr7ekZHfpqOsmv-JJJWhw" /><param name="isVid" value="true" /><param name="isUI" value="true" /><param name="dynamicStreaming" value="true" /><param name="@videoPlayer" value="' + BrightCoveVedioID + '" /></object>';
                        $("#brightcoveVideoPlaceholder").append(BCVideo);
                        brightcove.createExperiences();

                        $('#dvVidPresents').show();

                    }

                });
            }

        });

        //******************USER ALERT RELATED FUNCTIONS******************

        // *************** Start WT click events
        $(document).ready(function () {

            var locationInfo = window.location.hostname + window.location.pathname;
            var authentication = $("meta[name='DCSext.auth']").attr('content');
            var isMobile = $("meta[name='DCSext.mobilesite']").attr('content');

            $(".prev").click(function () {
                dcsMultiTrack('WT.es', locationInfo, 'WT.dl', '7', 'DCSext.buttonnav', 'Previous_Article_Button', 'DCSext.auth', authentication, 'DCSext.mobilesite', isMobile);
            });

            $(".next").click(function () {
                dcsMultiTrack('WT.es', locationInfo, 'WT.dl', '7', 'DCSext.buttonnav', 'Next_Article_Button', 'DCSext.auth', authentication, 'DCSext.mobilesite', isMobile);
            });

            $(".tab1Div > a").click(function () {
                dcsMultiTrack('WT.es', locationInfo, 'WT.dl', '7', 'DCSext.tabnav', 'Paper_Tab', 'DCSext.auth', authentication, 'DCSext.mobilesite', isMobile);
            });

            $(".tab5Div > a").click(function () {
                dcsMultiTrack('WT.es', locationInfo, 'WT.dl', '7', 'DCSext.tabnav', 'References_Tab', 'DCSext.auth', authentication, 'DCSext.mobilesite', isMobile);
            });

            $(".proceedingPDF .content_pdf").click(function () {
                dcsMultiTrack('WT.es', locationInfo, 'WT.dl', '7', 'WT.cg_n', 'MainDrillDown:Digital_Library;ProductTypeDrillDown;;Action;Success_Event', 'WT.cg_s', 'Main:Product_Details;Product:Proceedings;;Action:Download_PDF_Link;Success:PDF_Download', 'DCSext.auth', authentication, 'DCSext.mobilesite', isMobile);
            });

        });

        // *************** End WT click events
    </script>

<script type="text/javascript">
    function OpenLink(url) {
        var popUpUrl = null;
        if (url != null) {
            popUpUrl = url;
        } else {
            popUpUrl = "/store/Home.aspx";
        }
        if (window.opener != null && !window.opener.closed) {
            // for signpopup.aspx. this is a popup
            window.opener.location.href = popUpUrl;
            window.close();
        } else {
            // personalsignin.aspx. this is not a popup
            window.location.href = popUpUrl;
        }
    }

    function ShowProgressAnimation() {
        var pleaseWaitDialog = $("#progressBar").clone().removeClass('hide').dialog(
        {
            resizable: false,
            height: 'auto',
            width: 300,
            height: 100,
            modal: true,
            closeText: '',
            bgiframe: true,
            closeOnEscape: false,
            open: function (type, data) {
                $(this).parent().appendTo($("form:first"));
                $('body').css('overflow', 'auto'); //IE scrollbar fix for long checklist templates
            }
        });
    }

</script>


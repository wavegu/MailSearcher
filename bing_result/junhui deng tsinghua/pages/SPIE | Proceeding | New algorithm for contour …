            services_exclude: "email"
        };
        // Tracking share clicks
        addthis.addEventListener('addthis.menu.share', eventhandler);
        function eventhandler(evt) {
            var value = evt.data.service;
            var key = 'Share';
            this.UserSessionItem = new UserSessionItem("puntknpwe5lvcjguzpdzgqqw", 0, 0);
            this.ActionType = new ActionType(1, key);
            doTracking(this.ActionType, value.toString(), "10/28/2015 3:52:54 AM", this.UserSessionItem, (window.location).toString());
        }
